//posibles combinaciones con array



//-> Declaracion
let fruits1: string[] = ["hola","moises"];
let fruits2: number[] = [12,1,2];
let fruits3: boolean[] = [true, false];


let fruit1: Array<string> = ["hola","moises"];
let fruit2: Array<number> = [12,1,2];
let fruit3: Array<boolean> = [true, false];

let sfdsa: string[];
let sdsadsa: Array<string>;




//-> Asignacion



fruits1[1]= ["sfdsa"];
fruit2=["hola","moises"];
